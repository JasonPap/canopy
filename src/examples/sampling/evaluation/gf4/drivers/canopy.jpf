@using canopy

target=sampling.evaluation.gf4.drivers.HashMapDriverInt
target.args=10

shell=edu.cmu.sv.isstac.sampling.mcts.MCTSShell

symbolic.dp=z3bitvectorinc
listener=gov.nasa.jpf.symbc.numeric.solvers.IncrementalListener

symbolic.method=sampling.engagement1.gf4.hashmap.HashMap.get(con),sampling.engagement1.gf4.hashmap\
