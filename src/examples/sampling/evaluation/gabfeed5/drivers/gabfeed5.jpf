@using canopy
@using jpf-symbc

#shell=edu.cmu.sv.isstac.sampling.montecarlo.MonteCarloShell
#shell=edu.cmu.sv.isstac.sampling.mcts.MCTSShell
shell=edu.cmu.sv.isstac.sampling.exhaustive.ExhaustiveShell

symbolic.security.sampling.mcts.uct.bias=7

#symbolic.security.sampling.termination.samplingsize=4000
symbolic.security.sampling.seed=-135922421

target=sampling.evaluation.gabfeed5.drivers.MainSort

target.args=6
#symbolic.dp=z3bitvector
#symbolic.dp=z3
symbolic.dp=z3inc
listener=gov.nasa.jpf.symbc.numeric.solvers.IncrementalListener
#symbolic.bvlength=64

# range: A-Z (65-90)
symbolic.min_char=65 
symbolic.max_char=67

#symbolic.debug = true

symbolic.method=sampling.evaluation.gabfeed5.sort.Sorter.sort(con)

