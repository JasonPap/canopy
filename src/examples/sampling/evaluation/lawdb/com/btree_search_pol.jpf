@using spf-wca
@using canopy

shell=wcanalysis.WorstCaseAnalyzer

symbolic.dp=z3
#symbolic.worstcase.heuristic.nosolver=true

# Set history size for the stateful decision policy
symbolic.wc.policy.history.size=0
#symbolic.worstcase.reusepolicy=true
#symbolic.worstcase.startat=101

symbolic.worstcase.policy.inputsize=3
symbolic.worstcase.input.max=40
#symbolic.worstcase.input.stepsize=20

# output
symbolic.worstcase.verbose=true
symbolic.worstcase.outputpath=${canopy}/btree_search_pol_size_test_order10

# analysis setup
target=sampling.evaluation.lawdb.com.btree.drivers.DriverSearch
symbolic.method=sampling.evaluation.lawdb.com.btree.BTree.searchRange(con#con);com.btree.BTree.addMask(con#con#con)
symbolic.heuristic.measuredmethods=sampling.evaluation.lawdb.com.btree.BTree.searchRange(con#con)
